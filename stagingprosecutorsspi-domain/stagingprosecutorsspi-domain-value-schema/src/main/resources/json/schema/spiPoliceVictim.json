    "personGivenName1": {
      "type": "string"
    },
    "personGivenName2": {
      "type": "string"
    },
    "personGivenName3": {
      "type": "string"
    },
    "personFamilyName": {
      "type": "string"
    },
    "address": {
      "$ref": "http://cpp.moj.gov.uk/staging/prosecutors/spi/json/schemas/address.json"
    },
    "telephoneNumberBusiness": {
      "type": "string"
    },
    "telephoneNumberHome": {
      "type": "string"
    },
    "telephoneNumberMobile": {
      "type": "string"
    },
    "emailAddress1": {
      "type": "string"
    },
    "emailAddress2": {
      "type": "string"
    },
    "birthDate": {
      "type": "string",
