{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "id": "http://cpp.moj.gov.uk/staging/prosecutors/spi/json/schemas/system-Codes.json",
  "type": "object",
  "description": "CJS System Codes",
  "properties": {
    "id": {
      "$ref": "http://justice.gov.uk/domain/core/common/definitions.json#/definitions/uuid"
    },
    "seqNum": {
      "type": "integer"
    },
    "systemCode": {
      "type": "string"
    },
    "systemCodeDescription": {
      "type": "string"
    },
    "spiInFlag": {
      "type": "boolean"
    },
    "validFrom": {
      "type": "string"
    }
  },
  "required": [
    "id",
    "seqNum",
    "systemCode",
    "systemCodeDescription",
    "spiInFlag"
  ],
  "additionalProperties": false
}